<scroll-view
  class="videoContainer"
>
  <!-- 暴露的属性有：roomImg、roomName、videoSrc、authorImg、author、btnIsShow  -->
  <videoList-item
    wx:for="{{MovieList}}"
    roomName="{{item.name}}"
    roomImg="{{item.imagePath}}"
    author="{{item.comment}}"
    btnIsShow="{{item.isHighlyRecommended}}"
  />
</scroll-view>