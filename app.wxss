/**app.wxss**/
@import './res/my_animation.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clearfix::after {
  display: block;
  content: '';
  height: 0;
  visibility: hidden;
  clear: both;
}
