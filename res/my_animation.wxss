.shake-up {
    animation: shakeUp 4s cubic-bezier(0.42,0,0.58,1) infinite;
}
.shake-up1 {
    animation: shakeUp 4s cubic-bezier(0.42,0,0.58,1) infinite 0.5s;
}
.img-switch {
    animation: imgSwitch 4s cubic-bezier(0.42,0,0.58,1) infinite;
}

.fly-all1 {
    animation-name:flyAll1, shakeUp;
    animation-duration:1s, 4s;
    animation-timing-function:cubic-bezier(0,0,0.58,1), cubic-bezier(0.42,0,0.58,1);
    /*多个动画执行的时候 animation-delay无效只在第一次生效*/
    animation-delay:0, 1s;
    animation-iteration-count:1, infinite;
    /*animation-fill-mode:forwards, forwards;*/
}
.fly-all2 {
    animation-name:flyAll2, shakeUp;
    animation-duration:1s, 4s;
    animation-timing-function:cubic-bezier(0,0,0.58,1), cubic-bezier(0.42,0,0.58,1);
    animation-delay:0, 1s;
    animation-iteration-count:1, infinite;
    animation-fill-mode:forwards, forwards;
}
.fly-all3 {
    animation-name:flyAll3, shakeUp2;
    animation-duration:1s, 4s;
    animation-timing-function:cubic-bezier(0,0,0.58,1), cubic-bezier(0.42,0,0.58,1);
    animation-delay:0, 1s;
    animation-iteration-count:1, infinite;
    animation-fill-mode:forwards, forwards;
}
.fly-all4 {
    animation-name:flyAll4, shakeUp2;
    animation-duration:1s, 4s;
    animation-timing-function:cubic-bezier(0,0,0.58,1), cubic-bezier(0.42,0,0.58,1);
    animation-delay:0, 3s;
    animation-iteration-count:1, infinite;
    animation-fill-mode:forwards, forwards;
}
@keyframes rotate {
    0% {
        transform: none;
        transform-origin: none;
    }
    100% {
        transform: rotate(-1turn);
        transform-origin: center;
    }
}
@keyframes zoomIn {
    0% {
        opacity: 0.6;
        -webkit-transform: scale(.7);
        transform: scale(.7);
    }
    50% {
        opacity: 1;
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    100% {
        opacity: 0.6;
        -webkit-transform: scale(.7);
        transform: scale(.7);
    }
}
@keyframes wordsSwitch {
       0% {
        opacity: 0;
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    50% {
        opacity: 1;
        -webkit-transform: scale(1.3);
        transform: scale(1.3);
    }
    100% {
        opacity: 0;
        -webkit-transform: scale(1);
        transform: scale(1);
    } 
}
/*------闪烁放大------*/ 
@keyframes imgSwitch {
    0% {
        opacity: 0.6;
        -webkit-transform: scale(1);
        transform: scale(1) ;
    }
    50% {
        opacity: 1;
        -webkit-transform: scale(1.1);
        transform: scale(1.1);
    }
    100% {
        opacity: 0.6;
        -webkit-transform: scale(1);
        transform: scale(1);
    } 
}
/* 上下晃动*/ 
@keyframes shakeUp {
    0% {
        transform: translateY(0rem) ;
    }
    50% {
        transform: translateY(30rpx);
    }
    100% {
        transform: translateY(0rem);
    } 
}
@keyframes shakeUp2 {
    0% {
        transform: translateY(0rem) ;
    }
    50% {
        transform: translateY(-30rpx);
    }
    100% {
        transform: translateY(0rem);
    } 
}
@keyframes flyAll1 {
    0% {
    top: 3.84rem;
    left: 4.533333rem;
    }
    100% {
        top: 1.733333rem;
        left: 0.906667rem;
    } 
}
@keyframes flyAll2 {
    0% {
    top: 3.84rem;
    right: 2.4rem;
    }
    100% {
        top: 2.466667rem;
        right: 1.333333rem;
    } 
}
@keyframes flyAll3 {
    0% {
    top: 3.84rem;
    left: 4.533333rem;
    }
    100% {
        left: 1.0rem;
        top: 6.666667rem;
    } 
}
@keyframes flyAll4 {
    0% {
    top: 3.84rem;
    right: 2.4rem;
    }
    100% {
    right: 0.466667rem;
    top: 7.333333rem;
    } 
}

